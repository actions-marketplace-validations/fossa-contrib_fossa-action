{
  "name": "fossa-action",
  "version": "1.1.2",
  "private": true,
  "description": "Actions for running FOSSA scans",
  "author": "Sora Morimoto <sora@morimoto.io>",
  "license": "ISC",
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "build": "esbuild src/index.ts --bundle --outdir=dist --target=node12 --platform=node"
  },
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0",
    "@actions/tool-cache": "1.6.0"
  },
  "devDependencies": {
    "@types/node": "14.14.3",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "esbuild": "0.7.21",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  }
}
